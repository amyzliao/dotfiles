    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "editor.minimap.enabled": false,
    "editor.tabSize": 2,
    "[css]": {
        "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
    },
    "editor.accessibilitySupport": "off",
    "[javascript]": {
        "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "security.workspace.trust.untrustedFiles": "open",
    "vscode-neovim.neovimExecutablePaths.linux": "",
    "vscode-neovim.neovimExecutablePaths.win32": "",
    "vscode-neovim.neovimInitVimPaths.darwin": "/Users/azl/.config/nvim/init.vim",
    "vscode-neovim.compositeKeys": {},
    "cmake.pinnedCommands": [
        "workbench.action.tasks.configureTaskRunner",
        "workbench.action.tasks.runTask"
    ],
    "terminal.integrated.defaultProfile.linux": "bash",
    "github.copilot.chat.terminalChatLocation": "quickChat",
    "github.copilot.advanced": {},
    "github.copilot.enable": {
        "*": false,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "terminal.integrated.defaultProfile.osx": "tmux",
    "zenMode.fullScreen": false,
    "zenMode.hideLineNumbers": false,
    "zenMode.showTabs": "none",
    "zenMode.silentNotifications": false,
    "zenMode.centerLayout": false,
    "[markdown]": {
        "editor.defaultFormatter": "mervin.markdown-formatter",
        "editor.quickSuggestions": {
            "other": true,
            "comments": true,
            "strings": true
        },
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "markup.bold.markdown",
                "settings": {
                    "foreground": "#C485BF",
                    "fontStyle": "bold",
                }
            },
            {
                "scope": "markup.italic.markdown",
                "settings": {
                    // dark mode
                    "foreground": "#D7BA7D",
                    // light mode
                    // "foreground": "#ba7506",
                    "fontStyle": "italic",
                }
            },
            {
                "scope": "punctuation.definition.list.begin.markdown",
                "settings": {
                    "foreground": "#097acb",
                }
            },
            {
                "scope": "markup.list",
                "settings": {
                    // dark mode: white
                    // "foreground": "#ffffff",
                    // light mode: black
                    // "foreground": "#000000",
                }
            },
            {
                "scope": "markup.inline.raw",
                "settings": {
                    "foreground": "#4EC9B0",
                }
            },
            {
                "scope": "heading.1.markdown entity.name.section.markdown, heading.1.markdown punctuation.definition.heading.markdown",
                "settings": {
                    // dark mode
                    "foreground": "#DCDCAA",
                    // light mode
                    // "foreground": "#acac14",
                    "fontStyle": "bold",
                }
            },
            {
                "scope": "heading.2.markdown entity.name.section.markdown, heading.2.markdown punctuation.definition.heading.markdown",
                "settings": {
                    // dark mode
                    "foreground": "#9CDCFE",
                    // light mode
                    // "foreground": "#3598ce",
                }
            },
            {
                "scope": "heading.3.markdown entity.name.section.markdown, heading.3.markdown punctuation.definition.heading.markdown",
                "settings": {
                    // dark mode
                    "foreground": "#B4CDA7",
                    // light mode
                    // "foreground": "#4fb419",
                }
            },
            {
                "scope": "heading.4.markdown entity.name.section.markdown, heading.4.markdown punctuation.definition.heading.markdown",
                "settings": {
                    // dark mode
                    "foreground": "#569CD6",
                    // light mode
                    // "foreground": "#3e4abc",
                }
            },
            {
                "scope": "heading.5.markdown entity.name.section.markdown, heading.5.markdown punctuation.definition.heading.markdown",
                "settings": {
                    // dark mode
                    // "foreground": "#569CD6",
                    // light mode
                    "foreground": "#2279c1",
                }
            },
            {
                "scope": "heading.6.markdown entity.name.section.markdown, heading.6.markdown punctuation.definition.heading.markdown",
                "settings": {
                    // dark mode
                    "foreground": "#569CD6",
                    // light mode
                    // "foreground": "#2279c1",
                }
            },
        ]
    }
}