        {
            "key": "m",
            "name": "minimap",
            "type": "command",
            "command": "editor.action.toggleMinimap"
        },
        {
            "key": "n",
            "name": "remove highlight",
            "type": "command",
            "command": "vscode-neovim.send",
            "args": ":noh<CR>"
        },
        {
            "key": "s",
            "name": "Search...",
